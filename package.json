{
  "name": "@masterclassio/mc-react-timeline",
  "version": "1.0.5",
  "description": "MasterClass legacy - ReactJs package for timeline editor tracks component",
  "main": "dist/index.js",
  "author": "Nick Mitchell",
  "license": "MIT",
  "scripts": {
    "build:package": "clear && webpack --mode production --config webpack.package.config.js",
    "build": "clear && webpack --mode production",
    "dev": "clear && cross-env NODE_ENV=development webpack serve --config webpack.dev.config.js",
    "start": "clear && webpack serve",
    "test": "clear && dotenv -e .env jest",
    "prepublish": "yarn build",
    "semantic-release": "semantic-release",
    "gaa": "git add -A",
    "gc:init": "git commit -m \"📦chore: init commit\"",
    "gc:feat": "git commit -m \"🍉feat: $1\"",
    "gc:docs": "git commit -m \"📘docs:$1\"",
    "gc:readme": "git commit -m \"📘docs: update readme\"",
    "echo": "echo \"🍉feat: $1\""
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/masterclassio/mc-react-timeline.git"
  },
  "dependencies": {
    "@emotion/css": "^11.9.0",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@types/node": "^17.0.43",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "clsx": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "short-uuid": "^4.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^27.5.1",
    "@types/uuid": "^8.3.4",
    "canvas": "^3.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "dotenv-cli": "^5.1.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^29.0.3",
    "style-loader": "^3.3.1",
    "ts-jest": "^28.0.3",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.2",
    "webpack": "^5.73.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1"
  }
}
